        <div class="page-content-area">
            <div class="row">
                <div class="col-xs-12">
                    <form class="form-horizontal" id="pollingForm" role="form">
                        <input type="hidden" id="id" name="id" value="$!{polling.id}">
                        <input type="hidden" id="fileIds" name="fileIds" value="$!{polling.fileIds}">

                        <div class="form-group col-xs-6">
                            <label class="col-xs-4 control-label no-padding-right" for="cId"><span class="red">*</span>
                                客户名称:</label>

                            <div class="col-xs-8">
                                <input type="text" id="cId" name="cId"
                                       class="col-xs-12 ui-autocomplete-input"
                                       value="$!{customerId}" #if($customerId) disabled #end
                                       placeholder="请选择客户"/>
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <label class="col-xs-4 control-label no-padding-right" for="phone"> 客户电话:</label>

                            <div class="col-xs-8">
                                <input type="text" id="phone" name="phone" value="$!{polling.phone}"
                                       class="col-xs-12 ui-autocomplete-input" readonly
                                       placeholder=""/>
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <label class="col-xs-4 control-label no-padding-right" for="address"> 客户地址:</label>

                            <div class="col-xs-8">
                                <input type="text" id="address" name="address" value="$!{polling.address}"
                                       class="col-xs-12 ui-autocomplete-input" readonly
                                       placeholder=""/>
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <label class="col-xs-4 control-label no-padding-right" for="contact"> 联系人:</label>

                            <div class="col-xs-8">
                                <input type="text" id="contact" name="contact" value="$!{polling.contact}"
                                       class="col-xs-12 ui-autocomplete-input" readonly
                                       placeholder=""/>
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <label class="col-xs-4 control-label no-padding-right" for="eId"><a
                                    class="red">*</a> 主机号码:</label>

                            <div class="col-xs-8">
                                <input type="text" id="eId" name="eId"
                                       class="col-xs-12 ui-autocomplete-input"
                                       value="$!{eId}" #if($eId) disabled #end
                                       placeholder="请选择主机号码"/>
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <label class="col-xs-4 control-label no-padding-right" for="eModel"> 设备型号:</label>

                            <div class="col-xs-8">
                                <input type="text" id="eModel" name="eModel" value="$!{polling.eModel}"
                                       class="col-xs-12 ui-autocomplete-input" readonly
                                       placeholder=""/>
                            </div>
                        </div>

                        <div class="form-group col-xs-6">
                            <label class="col-xs-4 control-label no-padding-right" for="fax">电机厂牌:</label>

                            <div class="col-xs-8">
                                <input type="text" id="motorBrand" name="motorBrand" value="$!{polling.motorBrand}"
                                       class="col-xs-12 ui-autocomplete-input" readonly
                                       placeholder=""/>

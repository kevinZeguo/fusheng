                            <label class="col-xs-6 control-label no-padding-right" for="gearSpeed">
                                电机是否定期检查、保养、加油:</label>

                            <div class="col-xs-6"><input type="radio" id="isMotorRegularlyNormal1"
                                                         name="isMotorRegularlyNormal" value="1"
                                #if($!{polling.isMotorRegularlyNormal} == 1) checked#end
                            > 是
                                <input type="radio" id="isMotorRegularlyNormal0" name="isMotorRegularlyNormal" value="0"
                                    #if($!{polling.isMotorRegularlyNormal} == 0) checked#end
                                > 否
                            </div>
                        </div>

                        <div class="form-group col-xs-12">
                            <label class="col-xs-2 control-label no-padding-right" for="engineerSuggest">
                                服务人员建议:</label>

                            <div class="col-xs-10">
                    <textarea id="engineerSuggest" name="engineerSuggest"
                              class="col-xs-12 ui-autocomplete-input">$!{polling.engineerSuggest}</textarea>
                            </div>
                        </div>
                        <div class="form-group col-xs-12">
                            <label class="col-xs-2 control-label no-padding-right" for="note">客户意见:</label>

                            <div class="col-xs-10">
                    <textarea id="feedback" name="feedback"
                              class="col-xs-12 ui-autocomplete-input">$!{polling.feedback}</textarea>
                            </div>
                        </div>
                        <div class="form-group col-xs-12">
                            <label class="col-xs-2 control-label no-padding-right" for="attitude">
                                服务态度是否良好:</label>

                            <div class="col-xs-10">
                                <input type="radio" id="attitude1" name="attitude" value="1"
                                    #if($!{polling.attitude} == 1) checked#end
                                > 满意
                                <input type="radio" id="attitude0" name="attitude" value="0"
                                    #if($!{polling.attitude} == 0) checked#end
                                > 不满意
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <label class="col-xs-4 control-label no-padding-right" for="serviceEngineer">
                                <span class="red">*</span> 服务工程师:</label>

                            <div class="col-xs-8">
                                <input type="text" id="serviceEngineer" value="$!{polling.serviceEngineer}"
                                       name="serviceEngineer"
                                       class="col-xs-12 ui-autocomplete-input"
                                       placeholder="请选择服务工程师"/>
                            </div>
                        </div>
                        <div class="form-group col-xs-6">
                            <label class="col-xs-4 control-label no-padding-right" for="serviceDate">
                                <span class="red">*</span> 服务日期:</label>

                            <div class="col-xs-8">
                                <input type="text" class="col-xs-12 ui-autocomplete-input" placeholder="请选择服务日期"
                                       id="serviceDate"
                                       name="serviceDate" value="$!{polling.serviceDate}"/>
                            </div>
                        </div>

                        <div class="form-group col-xs-12 clearfix form-actions align-right">
                            <button class="btn btn-sm btn-primary" type="submit" id="submit-btn" name="submit-btn">
                                保存
                            </button>
                            &nbsp; &nbsp; &nbsp;
                            <button class="btn btn-white btn-info " type="button"
                                    onclick="javaScript:window.history.go(-1)" id="closeAdd">
                                返回
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
